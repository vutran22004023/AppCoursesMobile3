{
  "expo": {
    "name": "CourseNiver",
    "slug": "course-niver",
    "version": "1.0.0",

    "scheme": "course-niver",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,

      "tsconfigPaths": true
    },

    "orientation": "portrait",
    "icon": "./assets/logo/brain.png",

    "userInterfaceStyle": "automatic",

    "splash": {
      "image": "./assets/logo/brain2.png",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/brain2.png",
        "backgroundColor": "#161622"
      }
    }
  }
}
